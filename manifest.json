{
  "name": "Avocado Bugfixes",
  "version": "0.0.3",
  "description": "Fix bugs in Avocado.io, since it is barely being maintained. Requests & contributions welcome.",
  "content_scripts": [
    {
      "matches": [
        "https://avocado.io/=/"
      ],
      "js": [ 
        "fix.js" 
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://avocado.io/=/"
  ],
  "icons": {
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "manifest_version": 2
}